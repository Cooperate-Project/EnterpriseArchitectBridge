<?xml version="1.0" encoding="utf-8"?>
<persistence-mapping xmlns="http://www.eclipse.org/emft/teneo"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	
	<epackage namespace-uri="de.cooperateproject.eabridge.eaobjectmodel.xcore">
	
		<eclass name="Attribute">
			<eattribute name="AttributeID">
				<column name="ID"/>
			</eattribute>
			<eattribute name="AttributeGUID">
				<column name="ea_guid"/>
			</eattribute>
			<property name="AttributeID">
          		<id/>
         	</property>
			<property name="Parent">
				<join-column name="Object_ID"/>
			</property>
		</eclass>	
		
		<eclass name="AttributeConstraint">
			<table name="t_attributeconstraints"/>
			<!--<property name="Attribute">
          		<id/>
         	</property> 
         	<property name="Constraint">
          		<id/> 
         	</property> -->
			<property name="ElementOfAttribute">
				<join-column name="Object_ID"/>
			</property>
			<property name="Attribute">
				<join-column name="ID"/>
			</property>
		</eclass>		
			
			
			
		<eclass name="Element">
			<table name="t_object"/>
		</eclass>
	</epackage>
</persistence-mapping>